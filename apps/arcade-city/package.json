{
  "name": "@arcadecity/arcade-city",
  "version": "6.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:dev-client": "expo start --dev-client",
    "dev": "yarn web --port 19001",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export": "npx expo export:web",
    "test": "jest --watchAll",
    "clear": "expo r -c",
    "fixdeps": "expo doctor --fix-dependencies",
    "build:development": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview --platform ios",
    "build:ios": "eas build -p ios --profile production",
    "build:android": "eas build -p android --profile beta5",
    "build:android:apk": "eas build -p android --profile beta5-apk",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "publish:default": "expo publish --release-channel default",
    "publish:ios": "yarn publish:ios:production",
    "publish:ios:production": "eas update --branch production -p ios --message 'Update'",
    "publish:ios:preview": "eas update --branch preview -p ios --message 'Update'",
    "publish:android": "expo publish --release-channel beta5"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@alephium/noble-secp256k1": "^1.5.5",
    "@arcadecity/ui": "*",
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo-google-fonts/lexend": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-spring/native": "9.5.2",
    "@sentry/react-native": "^4.1.3",
    "create-hash": "1.2.0",
    "expo": "~46.0.0-beta.6",
    "expo-application": "~4.2.2",
    "expo-asset": "~8.6.1",
    "expo-constants": "~13.2.2",
    "expo-crypto": "~11.0.0",
    "expo-dev-client": "~1.1.0",
    "expo-device": "~4.3.0",
    "expo-font": "~10.2.0",
    "expo-linking": "~3.2.1",
    "expo-random": "~12.3.0",
    "expo-splash-screen": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-updates": "~0.14.2",
    "expo-web-browser": "~11.0.0",
    "mobx": "6.6.1",
    "mobx-react-lite": "3.4.0",
    "mobx-state-tree": "5.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7",
    "sentry-expo": "~5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/create-hash": "^1.2.2",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "jest": "^26.6.3",
    "jest-expo": "~44.0.1",
    "react-test-renderer": "18.0.0",
    "typescript": "~4.3.5"
  },
  "private": true,
  "resolutions": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  }
}
